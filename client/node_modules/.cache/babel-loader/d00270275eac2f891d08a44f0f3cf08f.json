{"ast":null,"code":"import { mapActions } from \"vuex\";\nexport default {\n  name: \"ProductDta\",\n\n  data() {\n    return {\n      errors: {\n        name: \"\",\n        email: \"\",\n        password: \"\"\n      },\n      users: [],\n      user: {\n        id: \"\",\n        uid: \"\",\n        name: \"\",\n        email: \"\",\n        password: \"\",\n        create_at: \"\"\n      }\n    };\n  },\n\n  created() {\n    const userId = this.$route.params.id;\n    console.log(userId);\n\n    if (userId) {\n      this.getUser(userId);\n    }\n\n    this.getAll();\n  },\n\n  methods: { ...mapActions([\"\"]),\n\n    validate() {\n      let isValid = true;\n      this.errors = {\n        name: \"\",\n        email: \"\",\n        password: \"\"\n      };\n\n      if (!this.user.name) {\n        this.errors.name = \"Error name\";\n        isValid = false;\n      }\n\n      if (!this.user.email) {\n        this.errors.email = \"Error email\";\n        isValid = false;\n      }\n\n      if (!this.user.password) {\n        this.errors.password = \"Loi password\";\n        isValid = false;\n      }\n\n      return isValid;\n    },\n\n    isNumber(value) {\n      return /^\\d*$/.test(value);\n    },\n\n    async save() {\n      if (this.validate()) {\n        if (this.user.id) {\n          this.$request.put(`http://localhost:8000/api/products/${this.user.id}`, this.user).then(res => {\n            if (res.data.success) {\n              this.$router.push({\n                name: \"admin.managements.products\"\n              });\n              return;\n            }\n\n            alert(\"Errors\");\n          });\n          return;\n        }\n\n        await this.getAll();\n        let max = 0;\n        let newID = 0; // console.log(newID)\n\n        for (let i = 0; i < this.users.length; i++) {\n          console.log(this.users.length);\n\n          if (this.users[i].pid > max) {\n            max = this.users[i].pid;\n          } // return this.product.id\n\n        }\n\n        newID = max + 1;\n        this.user.uid = newID;\n        console.log(newID);\n\n        var md5 = require(\"md5\");\n\n        this.user.password = md5(this.user.password);\n        this.user.create_at = new Date().toLocaleString();\n        this.user.uid = newID;\n        this.$request.post(\"http://localhost:8000/api/users\", this.user).then(res => {\n          if (res.data.success) {\n            this.$router.push({\n              name: \"admin.login\"\n            });\n          }\n        });\n      }\n    },\n\n    getProduct(userId) {\n      this.$request.get(`http://localhost:8000/api/users/${userId}`).then(res => {\n        this.product = res.data; // console.log(this.product)\n      });\n    },\n\n    reset() {\n      this.user.name = \"\", this.user.email = \"\", this.user.password = \"\";\n    },\n\n    async getAll() {\n      console.log(this.builUrl());\n      await this.$request.get(this.builUrl()).then(res => {\n        this.products = res.data; // sort price\n        // this.products = this.products.sort((a, b) => a.price - b.price);\n        // sort reverse\n\n        this.user = this.user.reverse();\n\n        var md5 = require(\"md5\");\n\n        console.log(md5(\"message\"));\n      });\n    },\n\n    builUrl() {\n      // let p = this.params;\n      // return `http://localhost:8000/api/products?page=${p.page}&per_page=${p.per_page}&sort_column=${p.sort_column}&direction=${p.direction}&search_column=${p.search_column}&search_operator=${p.search_operator}&search_query_1=${p.search_query_1}&search_query_2=${p.search_query_2}`;\n      return `http://localhost:8000/api/products`;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiFA;AACA;EACAA,kBADA;;EAEAC;IACA;MACAC;QACAF,QADA;QAEAG,SAFA;QAGAC;MAHA,CADA;MAMAC,SANA;MAOAC;QACAC,MADA;QAEAC,OAFA;QAGAR,QAHA;QAIAG,SAJA;QAKAC,YALA;QAMAK;MANA;IAPA;EAgBA,CAnBA;;EAoBAC;IACA;IACAC;;IACA;MACA;IACA;;IACA;EACA,CA3BA;;EA4BAC,WACA,mBADA;;IAEAC;MACA;MACA;QACAb,QADA;QAEAG,SAFA;QAGAC;MAHA;;MAKA;QACA;QA