    console.log(this.password);\n      this.$request.post(\"http://localhost:8000/api/login\", {\n        email: this.email,\n        password: this.password\n      }).then(res => {\n        console.log(res);\n\n        if (!res.data.success) {\n          this.isError === true;\n          this.error = \"Email or Password is not correct\";\n          this.reset();\n          console.log(res.data.success);\n          console.log(res.status);\n        }\n\n        if (res.data.success) {\n          this.isError === false;\n          this.isSuccess === true;\n          console.log(res.data.login);\n          console.log(res.data.token);\n          console.log(res.data.user.email);\n          this.success = \"Successful Login!\";\n          localStorage.setItem(\"token\", res.data.token);\n          localStorage.setItem(\"email\", res.data.user.email);\n          localStorage.setItem(\"login\", \"online\");\n          this.user.name = res.data.user.name;\n          console.log(this.user.name);\n          this.user.token = localStorage.getItem(\"token\"); // this.$router.push({ name: \"users.list\" });\n          // this.reset();\n\n          window.location.href = \"/admin/management/users\"; // this.user.name = res.data.user.name;\n          // console.log(token);\n          // this.$router.push(\"/products\");\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmJA;AACA;EACAA;IACA;MACAC,SADA;MAEAC,cAFA;MAGAC,WAHA;MAIAC,gBAJA;MAKAC,MALA;MAMAC,YANA;MAOAC,SAPA;MAQAC,YARA;MASAC;QACAF,oCADA;QAEAG,oCAFA;QAGAJ;MAHA;IATA;EAeA,CAjBA;;EAkBAK;IACAC;IACA;EACA,CArBA;;EAsBAC,YAtBA;;EAwBAC;IACAC;MACAC;MACAA;MACAA;IACA,CALA;;IAMAC;MACAL;MACA,cACAM,IADA,CACA,kCADA,EACA;QACAX,sBADA,CAGA;;MAHA,CADA,EAMAY,IANA,CAMAC;QACA;QAEA;UACAR;UACAA;QACA;;QAEA;UACA;UACAA,iCAFA,CAGA;;UACAI;UACA;UACA,4CANA,CAQA;UACA;UACA;UACA;UACA;UACA;QACA;MACA,CA7BA;IA8BA,CAtCA;;IAuCAK;MACA;IACA,CAzCA;;IA0CAC;MACA;MAEA;MACA;MACA;;MACA;MACAV;MACA,cACAM,IADA,CACA,iCADA,EACA;QACAX,iBADA;QAEAC;MAFA,